b.pdf: beamercolorthemeLUH.sty beamerinnerthemeLUH.sty beamerouterthemeLUH.sty beamerthemeLUH.sty coinbasgauche.jpg transparents.tex hautp1.png haut.png logoAPT.png logoINRA.png Makefile README.md traitbas.png traitgauche.png bibliography.bib beamerthemeUMR1145.tex
	cp transparents.bbl transparents.bblcopy || pdflatex transparents || { rm -i *.aux ; exit 1 ; } ; biber transparents ; pdflatex transparents || rm -i *.aux ; while grep "`diff transparents.bbl transparents.bblcopy > /dev/null && echo '^LaTeX Warning: Label(s) may have changed. Rerun to get \\(cross-references\\|the bars\\) right'`." transparents.log ; do cp transparents.bbl transparents.bblcopy ; pdflatex transparents || rm -i *.aux ; done ; cp transparents.pdf b.pdf
diff.pdf: avant.tex b.pdf
	for i in .* ; do cp $$i diff$${i#} ; done ; latexdiff -tCTRADITIONAL avant.tex transparents.tex | sed -e 's:\.pdf}:.eps}:g' -e 's:\\let\\commentfor\\latexdiff :%\\let\\commentfor\\latexdiff :g' -e 's/\[\.\.\\footnote{removed: #1} ]/\\ifmmode\\footnote{$$ #1$$ }\\else\\footnote{#1}\\fi/' -e 's:\\label{:\\label{replacedByOldValue:g' -e 's:\\begin{figure}:&\\begin{minipage}{\\textwidth}:' -e 's:\\begin{table}:&\\begin{minipage}{\\textwidth}:' -e 's:\\end{figure}:\\end{minipage}&:' -e 's:\\end{table}:\\end{minipage}&:' -e 's:stable]{footmisc}:para]{footmisc}:g' > diff.tex ; echo q | latex diff.tex ; dvipdf diff.dvi diff.pdf
